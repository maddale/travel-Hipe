

<li>
	<%= link_to gravatar_for(user, 82), user_path(user) %>
	<strong><%= link_to user.name, user_path(user) %></strong><br>
	<small><%= user.email %></small>
	<br>
		<small><small><em>Signed Up since: <%= user.created_at %></em></small>
			<br>
			<%= link_to "View", user_path(user) %> 
				<% if user_admin? || current_user == user %>
				| <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Are yoy sure?"} %>
				<% end %>
			</small>
		
</li>




